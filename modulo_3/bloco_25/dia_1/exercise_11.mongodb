use("erp");
db.vendas.aggregate([
  {$match: {
    dataVenda: {
      $gte: ISODate('2020-01-01'),
      $lte: ISODate('2020-03-31'),
    }
  }}, 
  {/**
   * _id: The id of the group.
   * fieldN: The first field name.
   */
  $group: {
    _id: "$clienteId",
    quantidadeCompra: {
      $sum: 1,
    }
  }},
  {/**
   * query: The query in MQL.
   */
  $match: {
    quantidadeCompra: {$lt: 3}
  }}
])